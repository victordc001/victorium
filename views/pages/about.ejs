    <!-- Start About Area -->
    <section class="about section">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-6 col-12">
                    <div class="image wow fadeInUp" data-wow-delay=".4s">
                        <img src="assets/images/about/about-image.png" alt="#">
                    </div>
                </div>
                <div class="col-lg-6 col-12">
                    <div class="content wow fadeInUp" data-wow-delay=".7s">
                        <h4>DECENTRALIZED FINANCIAL SOLUTIONS</h4>
                        <h2>Welcome to Victorium - Your Gateway to Seamless Crypto Tracking</h2>
                        <p>Victorium is your trusted platform for tracking cryptocurrency transactions across multiple networks. With a focus on transparency and accuracy, we help users stay informed about their digital asset movements in real time. Join us as we simplify crypto tracking and set the foundation for a smarter decentralized future.</p>
                           
                        <div class="list">
                            <div class="single-list">
                                <div class="list-icon">
                                    <i class="lni lni-checkmark"></i>
                                </div>
                                <h4>Secure Wallet Integration</h4>
                                <p>Connect your wallet easily and manage your crypto assets with confidence.</p>
                            </div>
                            <div class="single-list">
                                <div class="list-icon">
                                    <i class="lni lni-checkmark"></i>
                                </div>
                                <h4>Real-Time Transaction Tracking</h4>
                                <p>Monitor your transactions across multiple networks with instant updates.</p>
                            </div>
                            <div class="single-list">
                                <div class="list-icon">
                                    <i class="lni lni-checkmark"></i>
                                </div>
                                <h4>Multi-Network Support</h4>
                                <p>Choose from Bitcoin, Ethereum, and Solana networks for seamless transfers.</p>
                            </div>
                        </div>
                        
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End About Area -->
  

       <!-- Start Get Started Process Area -->
<section class="start-process dark section">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="section-title">
                    <h3 class="wow zoomIn" data-wow-delay=".2s">Ready to Join Victorium?</h3>
                    <h2 class="wow fadeInUp" data-wow-delay=".4s">3 Simple Steps to Begin</h2>
                    <p class="wow fadeInUp" data-wow-delay=".6s">Start your crypto journey with Victorium. Secure your wallet, make transactions, and explore limitless possibilities.</p>
                </div>
            </div>
        </div>
        <div class="row">
            <!-- Step 1 -->
            <div class="col-lg-4 col-md-4 col-12">
                <div class="single-process">
                    <span class="serial">01</span>
                    <h3>Connect Your Wallet</h3>
                    <p>Link your preferred crypto wallet to access Victorium's secure platform.</p>
                </div>
            </div>
            <!-- Step 2 -->
            <div class="col-lg-4 col-md-4 col-12">
                <div class="single-process">
                    <span class="serial">02</span>
                    <h3>Select Your Network</h3>
                    <p>Choose from Bitcoin, Ethereum, or Solana to start your transactions.</p>
                </div>
            </div>
            <!-- Step 3 -->
            <div class="col-lg-4 col-md-4 col-12">
                <div class="single-process">
                    <span class="serial">03</span>
                    <h3>Track & Manage Transactions</h3>
                    <p>Stay updated with real-time transaction tracking across multiple networks.</p>
                </div>
            </div>
        </div>
    </div>
</section>
