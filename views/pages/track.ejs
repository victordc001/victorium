      <%- include('../partials/head.ejs'); %>  

         <style>
              .gridding{ 
                  min-width:100px;
                  display:grid;
                  margin-top:20px;
                  margin-bottom:20px;
                  grid-gap:20px; 
                  grid-template-columns:repeat(auto, minmax(250px, 3fr)); 
              }  

              .gridding button{ 
                width:100%; 
                border:none;
                color:black;
                font-weight:bold;
                background-color:lightgrey; 
                transition:ease-in-out 0.3s ;
                padding:15px;
              }   

              .gridding button:hover{
                 background-color:grey; 
                 color:white;
              }

         </style>
</head>

